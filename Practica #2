#include "mbed.h"

DigitalOut myled(LED1);
DigitalOut myled2(LED2);
DigitalOut myled3(LED3);
int main() {
    
    
while(1){
        myled = 1;
        wait(0.2);
        myled = 0;
        wait(0.2);

        myled2=1;
        wait(0.2);
        myled2=0;
        wait(0.2);

        myled3=1;
        wait(0.2);
        myled3=0;
        wait(0.2);}
        
    }
